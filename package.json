{
  "name": "pos-system",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.dev.json",
    "start:dev": "ng serve --proxy-config proxy.conf.dev.json",
    "start:prod": "ng serve --proxy-config proxy.conf.prod.json",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:prod": "ng build --configuration=production",
    "deploy:dev": "npm run build:dev && firebase use dev && firebase deploy --only hosting:app",
    "deploy:prod": "npm run build:prod && firebase use prod && firebase deploy --only hosting:app",
    "build:analyze": "ng build --configuration=production && npx webpack-bundle-analyzer dist/pos-system/main*.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cap:sync": "npx cap sync",
    "cap:sync:android": "npx cap sync android",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android",
    "android:build": "ng build --configuration=production && npx cap sync android",
    "android:dev": "ng build --configuration=development && npx cap sync android && npx cap open android",
    "android:run": "ng build --configuration=production && npx cap sync android && npx cap run android"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/cdk": "^19.2.6",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.2.6",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "^6.0.1",
    "@capacitor/core": "^6.2.0",
    "@capacitor/filesystem": "^6.0.1",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/network": "^6.0.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "@capacitor-community/bluetooth-le": "^6.0.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "firebase": "^11.2.0",
    "jsbarcode": "^3.12.1",
    "lightningcss": "^1.30.1",
    "ngx-barcode6": "^1.20.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.0",
    "@capacitor/cli": "^6.2.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.3.0",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^11.1.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.7",
    "typescript": "~5.7.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
